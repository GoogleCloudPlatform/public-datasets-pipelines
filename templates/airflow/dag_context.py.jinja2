with DAG(
{%- for name, val in dag_init.items() %}
    {% if name == "dag_id" -%}
        dag_id="{{ namespaced_dag_id }}",
    {%- elif name == "default_args" -%}
        default_args=default_args,
    {%- else -%}
        {{ name|indent() }}={{ val|tojson if val is string else val }},
    {%- endif %}
{%- endfor %}
) as dag:
